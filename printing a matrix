# include <stdio.h>
#include<conio.h>
void main()
{
int a[10][10],r,c;
int i,j;
printf("enter the number of row & colum :");
scanf("%d%d",&r,&c);
printf("enter the elements of matrix:");
for(i=0;i<r;i++)
for(j=0;j<c;j++)
{
    scanf("%d",&a[i][j]);
}

for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
    printf("%d",a[i][j]);
}
printf("\n");
}
}
